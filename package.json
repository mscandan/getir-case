{
  "name": "getir-case",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-paginate": "^8.1.3",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@react-theming/storybook-addon": "^1.1.7",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/node-logger": "^6.5.12",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.19",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-icons": "^3.0.0",
    "@types/react-paginate": "^7.1.1",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "babel-jest": "^29.0.3",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "react-test-renderer": "^18.2.0",
    "stylelint": "^14.12.1",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^29.0.2",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "yarn jest",
    "eject": "react-scripts eject",
    "type-check": "yarn tsc --noEmit",
    "eslint": "eslint ./src --ext .jsx,.ts,.tsx,.js --cache",
    "prettier": "prettier \"src/**/*\"",
    "eslint:fix": "yarn eslint --fix",
    "prettier:fix": "yarn prettier --check",
    "lint:css": "stylelint './src/**/*.tsx' --cache --cache-location '.stylelintcache'",
    "lint": "yarn eslint && yarn prettier --list-different && yarn lint:css",
    "lint:fix": "yarn eslint:fix && yarn prettier:fix && yarn lint:css",
    "check-rule": "yarn type-check && yarn lint && yarn test",
    "storybook": "start-storybook -p 6006 public",
    "prepare": "husky install",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,scss,md}": [
      "yarn lint:fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
